<body id="my-home">
<div class="davcai-header" id="ajax_login_msg" _weibouserId="$!user.weiboUserId">
	<div class="davcai-header-content">
		<div id="sync_head">
			<h1 id="davcai-logo">
				<a href="http://www.davcai.com/"></a>
			</h1>
            <p class="davcai-address">
	         	<a class="huoban">新浪微博官方合作伙伴</a>
            </p>
		</div>
		<form class="login-form">
  	 	 	<input type="text" id="head-search"  value="搜索  讨论/用户" autocomplete="off" maxlength=""
  	 	 		onfocus="this.value=''" onblur="if(this.value==''){this.value='搜索  讨论/用户'}"/>
  	 	 	<span class="magnifier"><a href="javascript:void(0);"></a></span>
			<ul class="searching-dropdown-menu" id="search_result">
			</ul>
		</form>
	</div>
</div>