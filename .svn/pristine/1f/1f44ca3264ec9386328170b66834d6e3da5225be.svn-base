<!--微博头引用的css和js start-->
#set($weibo = "http://www.davcai.com/")
<link rel="stylesheet" type="text/css" href="$weibo/css/bootstrap.css" />
<link rel="stylesheet" type="text/css" href="$weibo/css/home.css" />
<link type="text/css" rel="stylesheet" href="$weibo/css/layout.css" />
<link rel="stylesheet" type="text/css" href="$weibo/css/header_login.css" />
<link rel="stylesheet" type="text/css" href="$weibo/css/davcai/header/header.css" />
<script type="text/javascript" src="$weibo/js/jet.js?v=$resVersoin"></script>
<script type="text/javascript" src="$weibo/js/web/placeholder.js"></script>
<script type="text/javascript" src="$weibo/js/web/load-header.js"></script>
<script type="text/javascript" src="$weibo/js/web/common.js"></script>
<script type="text/javascript" src="$weibo/js/ui.core.js"></script>
