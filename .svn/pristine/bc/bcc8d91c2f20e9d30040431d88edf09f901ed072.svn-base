package com.unison.lottery.weibo.dataservice.parse.model;

import java.math.BigDecimal;
import java.util.Date;
import org.apache.commons.lang3.builder.ToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;

/**
 * @desc 篮球大小球赔率
 * @author lei.li@unison.net.cn
 * @createTime 2014-3-12
 * @version 1.0
 */
public class BBOddsBigData {

	private long matchId;

	private long corpId;

	private BigDecimal initHandicap = BigDecimal.ZERO;

	private BigDecimal initBigOdds  = BigDecimal.ZERO;

	private BigDecimal initSmallOdds = BigDecimal.ZERO;

	private BigDecimal realtimeHandicap = BigDecimal.ZERO;

	private BigDecimal realtimeBigOdds = BigDecimal.ZERO;

	private BigDecimal realtimeSmallOdds = BigDecimal.ZERO;

	private BigDecimal groundHandicap = BigDecimal.ZERO;

	private BigDecimal groundBigOdds = BigDecimal.ZERO;

	private BigDecimal groundSmallOdds = BigDecimal.ZERO;

	private Date createTime;

	private Date updateTime;

	
	public long getMatchId() {
		return matchId;
	}

	public void setMatchId(long matchId) {
		this.matchId = matchId;
	}

	public long getCorpId() {
		return corpId;
	}

	public void setCorpId(long corpId) {
		this.corpId = corpId;
	}

	public BigDecimal getInitHandicap() {
		return initHandicap;
	}

	public void setInitHandicap(BigDecimal initHandicap) {
		this.initHandicap = initHandicap;
	}

	public BigDecimal getInitBigOdds() {
		return initBigOdds;
	}

	public void setInitBigOdds(BigDecimal initBigOdds) {
		this.initBigOdds = initBigOdds;
	}

	public BigDecimal getInitSmallOdds() {
		return initSmallOdds;
	}

	public void setInitSmallOdds(BigDecimal initSmallOdds) {
		this.initSmallOdds = initSmallOdds;
	}

	public BigDecimal getRealtimeHandicap() {
		return realtimeHandicap;
	}

	public void setRealtimeHandicap(BigDecimal realtimeHandicap) {
		this.realtimeHandicap = realtimeHandicap;
	}

	public BigDecimal getRealtimeBigOdds() {
		return realtimeBigOdds;
	}

	public void setRealtimeBigOdds(BigDecimal realtimeBigOdds) {
		this.realtimeBigOdds = realtimeBigOdds;
	}

	public BigDecimal getRealtimeSmallOdds() {
		return realtimeSmallOdds;
	}

	public void setRealtimeSmallOdds(BigDecimal realtimeSmallOdds) {
		this.realtimeSmallOdds = realtimeSmallOdds;
	}

	public BigDecimal getGroundHandicap() {
		return groundHandicap;
	}

	public void setGroundHandicap(BigDecimal groundHandicap) {
		this.groundHandicap = groundHandicap;
	}

	public BigDecimal getGroundBigOdds() {
		return groundBigOdds;
	}

	public void setGroundBigOdds(BigDecimal groundBigOdds) {
		this.groundBigOdds = groundBigOdds;
	}

	public BigDecimal getGroundSmallOdds() {
		return groundSmallOdds;
	}

	public void setGroundSmallOdds(BigDecimal groundSmallOdds) {
		this.groundSmallOdds = groundSmallOdds;
	}

	public Date getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}

	public Date getUpdateTime() {
		return updateTime;
	}

	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}

	public String toString() {
		return ToStringBuilder.reflectionToString(this,
				ToStringStyle.MULTI_LINE_STYLE);
	}
}
