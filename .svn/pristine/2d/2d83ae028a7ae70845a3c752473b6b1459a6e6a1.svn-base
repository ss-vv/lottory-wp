package com.unison.lottery.weibo.dataservice.parse.model;

import java.util.List;

import org.apache.commons.lang3.builder.ReflectionToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;

/**
 * 足球赔率
 */
public class CurrentDayOddsData {
	
	private List<League> leagueDatas;

	private List<MatchProcess> matchProcessDatas;
	
	private List<AsiaOdd> asiaOddDatas;
	
	private List<EuropeOdd> europeOddDatas;
	
	private List<BigSmallOdd> bigSmallOddDatas;
	
	private List<HalfOdd> halfOddDatas;
	
	private List<HalfBigSmallOdd> halfBigSmallOddDatas;

	public List<League> getLeagueDatas() {
		return leagueDatas;
	}

	public void setLeagueDatas(List<League> leagueDatas) {
		this.leagueDatas = leagueDatas;
	}

	public List<MatchProcess> getMatchProcessDatas() {
		return matchProcessDatas;
	}

	public void setMatchProcessDatas(List<MatchProcess> matchProcessDatas) {
		this.matchProcessDatas = matchProcessDatas;
	}

	public List<AsiaOdd> getAsiaOddDatas() {
		return asiaOddDatas;
	}

	public void setAsiaOddDatas(List<AsiaOdd> asiaOddDatas) {
		this.asiaOddDatas = asiaOddDatas;
	}

	public List<EuropeOdd> getEuropeOddDatas() {
		return europeOddDatas;
	}

	public void setEuropeOddDatas(List<EuropeOdd> europeOddDatas) {
		this.europeOddDatas = europeOddDatas;
	}

	public List<BigSmallOdd> getBigSmallOddDatas() {
		return bigSmallOddDatas;
	}

	public void setBigSmallOddDatas(List<BigSmallOdd> bigSmallOddDatas) {
		this.bigSmallOddDatas = bigSmallOddDatas;
	}

	public List<HalfOdd> getHalfOddDatas() {
		return halfOddDatas;
	}

	public void setHalfOddDatas(List<HalfOdd> halfOddDatas) {
		this.halfOddDatas = halfOddDatas;
	}

	public List<HalfBigSmallOdd> getHalfBigSmallOddDatas() {
		return halfBigSmallOddDatas;
	}

	public void setHalfBigSmallOddDatas(List<HalfBigSmallOdd> halfBigSmallOddDatas) {
		this.halfBigSmallOddDatas = halfBigSmallOddDatas;
	}

	public String toString(){
		return ReflectionToStringBuilder.toString(this,
				ToStringStyle.MULTI_LINE_STYLE);
	}
}
