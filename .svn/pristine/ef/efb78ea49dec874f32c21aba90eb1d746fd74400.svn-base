package com.unison.lottery.weibo.data.service.store.data;

import java.math.BigDecimal;
import com.unison.lottery.weibo.data.service.store.persist.entity.BBLotteryCorpPO;

/**
 * 篮球大小球盘口，数据对象
 * 
 * @desc
 * @author lei.li@unison.net.cn
 * @createTime 2014-3-17
 * @version 1.0
 */
public class BBOddsBigSmallVO extends BBLotteryCorpPO {

	private int index;
	
	/** 亚盘初盘水位*/
	private BigDecimal initHandicap;	// 初盘盘口
	private BigDecimal initBigOdds;		// 初盘大分赔率
	private BigDecimal initSmallOdds; 	// 初盘小分赔率
	
	/** 亚盘即时盘水位*/
	private BigDecimal realtimeHandicap;	// 即时盘盘口
	private BigDecimal realtimeBigOdds;		// 即时盘大分赔率
	private BigDecimal realtimeSmallOdds;	// 即时盘小分赔率
	
	public int getIndex() {
		return index;
	}
	
	public void setIndex(int index) {
		this.index = index;
	}
	
	public BigDecimal getInitHandicap() {
		return initHandicap;
	}
	
	public void setInitHandicap(BigDecimal initHandicap) {
		this.initHandicap = initHandicap;
	}

	
	public BigDecimal getInitBigOdds() {
		return initBigOdds;
	}

	
	public void setInitBigOdds(BigDecimal initBigOdds) {
		this.initBigOdds = initBigOdds;
	}

	
	public BigDecimal getInitSmallOdds() {
		return initSmallOdds;
	}

	
	public void setInitSmallOdds(BigDecimal initSmallOdds) {
		this.initSmallOdds = initSmallOdds;
	}

	
	public BigDecimal getRealtimeHandicap() {
		return realtimeHandicap;
	}

	
	public void setRealtimeHandicap(BigDecimal realtimeHandicap) {
		this.realtimeHandicap = realtimeHandicap;
	}

	
	public BigDecimal getRealtimeBigOdds() {
		return realtimeBigOdds;
	}

	
	public void setRealtimeBigOdds(BigDecimal realtimeBigOdds) {
		this.realtimeBigOdds = realtimeBigOdds;
	}

	
	public BigDecimal getRealtimeSmallOdds() {
		return realtimeSmallOdds;
	}

	
	public void setRealtimeSmallOdds(BigDecimal realtimeSmallOdds) {
		this.realtimeSmallOdds = realtimeSmallOdds;
	}
}