.instant-index-td {
	position: relative;
}
.instant-index-list {
	width: 99%;
	height: auto;
	border: 1px #EE8A85 solid;
	position: absolute;
	top: -2px;
	left: 0;
	z-index: 99;
	background: #fff;
	display: none;
}
.instant-index-list a.current {
	font-weight: bold;
}
