	text-align: left;
	padding-left: 5px;
	padding-right: 5px;
	font-size: 12px;
	margin-bottom: 3px;
}
.instant-index-list-content {
	clear: both;
	width: 97%;
	margin: 0 auto;
	height: 100%;
}
.instant-index-list-content li span{
	cursor: pointer;
}
@-moz-document url-prefix() {
	.instant-index-list-content {
		width: 98%;
	}
}
.instant-index-list-content li {
	border-bottom: 1px #E74C3C dashed;
	height: auto;
	overflow: hidden;
	min-height: 21px;
	padding-bottom: 10px;
	margin-bottom: 15px;
}
.instant-index-list-content li:last-child {
	border: none;
}
.instant-index-list-content li span {
	display: inline-block;
	width: 60px;
	padding: 2px 2px 2px 2px;
	font-size: 12px;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	margin-right: 5px;
	margin-left: 5px;
	text-align: center;
	float: left;
	margin-top: 5px;
}
.instant-index-list-content li span.current {
	background: #E74C3C;
}
.instant-index-list-content li span.current a {
	color: #fff;
}
.instant-index-list-content li span:hover {
	background: #E74C3C;
}
.instant-index-list-content li span:hover a {
	color: #fff;
}
