package com.unison.lottery.weibo.dataservice.commons.model;

import org.apache.commons.lang3.builder.ReflectionToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;

/**
 * 比赛历史数据
 * 
 * @author Yang Bo
 */
public class MatchHistory {

	private String matchId;				// 球探网比赛id
	private String sclassID;			// 所属联赛id
	private String homeTeamId;			// 主队id
	private String guestTeamId;			// 客队id
	private String homeTeamName;		// 主队名
	private String guestTeamName;		// 客队名
	
	private Object[][] againstHistory;	// 对赛往绩
	private Object[][] hostNearScore;	// 主队近期战绩
	private Object[][] guestNearScore;	// 客队近期战绩
	
	private Object[][] opOdds;			// 欧赔
	private Object[][] ypOdds;			// 亚赔
	private Object[][] jcOdds;			// 竞彩赔率
	

	public String getMatchId() {
		return matchId;
	}
	public void setMatchId(String matchId) {
		this.matchId = matchId;
	}
	public String getSclassID() {
		return sclassID;
	}
	public void setSclassID(String sclassID) {
		this.sclassID = sclassID;
	}
	public String getHomeTeamId() {
		return homeTeamId;
	}
	public void setHomeTeamId(String homeTeamId) {
		this.homeTeamId = homeTeamId;
	}
	public String getGuestTeamId() {
		return guestTeamId;
	}
	public void setGuestTeamId(String guestTeamId) {
		this.guestTeamId = guestTeamId;
	}
	public String getHomeTeamName() {
		return homeTeamName;
	}
	public void setHomeTeamName(String homeTeamName) {
		this.homeTeamName = homeTeamName;
	}
	public String getGuestTeamName() {
		return guestTeamName;
	}
	public void setGuestTeamName(String guestTeamName) {
		this.guestTeamName = guestTeamName;
	}
	public Object[][] getAgainstHistory() {
		return againstHistory;
	}
	public void setAgainstHistory(Object[][] againstHistory) {
		this.againstHistory = againstHistory;
	}
	public Object[][] getHostNearScore() {
		return hostNearScore;
	}
	public void setHostNearScore(Object[][] hostNearScore) {
		this.hostNearScore = hostNearScore;
	}
	public Object[][] getGuestNearScore() {
		return guestNearScore;
	}
	public void setGuestNearScore(Object[][] guestNearScore) {
		this.guestNearScore = guestNearScore;
	}
	public Object[][] getOpOdds() {
		return opOdds;
	}
	public void setOpOdds(Object[][] opOdds) {
		this.opOdds = opOdds;
	}
	public Object[][] getYpOdds() {
		return ypOdds;
	}
	public void setYpOdds(Object[][] ypOdds) {
		this.ypOdds = ypOdds;
	}
	public Object[][] getJcOdds() {
		return jcOdds;
	}
	public void setJcOdds(Object[][] jcOdds) {
		this.jcOdds = jcOdds;
	}
	public String toString(){
		return ReflectionToStringBuilder.toString(this, ToStringStyle.MULTI_LINE_STYLE);
	}
}
