package com.xhcms.lottery.pb.exception;

public class PartnerBetException extends PartnerException {

	private static final long serialVersionUID = 1L;
	
	public PartnerBetException(String msg, int errorCode){
		super(msg);
		this.setErrorCode(errorCode);
	}
	public PartnerBetException(String msg, Throwable cause){
		super(msg,cause);
	}
	public PartnerBetException(String msg){
		super(msg);
	}
}	
