			<script type="text/javascript">
				$(function() {
					ajax_bd_matchs(_issueNumber);
				});
				
				var ajax_bd_matchs = function(issueNumber) {
					new PassUI({
						time : $('#timeTabs'),
						grid : $('#matchGrid'),
						panel : $('#betPanel'),
						maxMc : 15,
						minBetMc : 3,
						strategy : new JCBetStrategy(),
						renderer : new BD_SF_Renderer({'playId':'96_BJDC_SF'}),
						url : 'http://trade.davcai.com/df/aj_bdlist.do',
						playId : '96_BJDC_SF',
						issueNumber : issueNumber
					});
				};
			</script>
