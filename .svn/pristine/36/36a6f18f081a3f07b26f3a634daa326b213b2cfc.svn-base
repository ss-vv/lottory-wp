									<form _n="form" action="http://trade.davcai.com/ac/bet.do"
										method="post">
										<input name="issueNumber" type="hidden" value=""/>
										<input name="matchs" type="hidden" />
										<input name="passTypes" type="hidden" value="1@1" />
										<input name="playId" type="hidden" />
										<input name="matchAnnotation" type="hidden" />
										<label class="lbl">投注倍数：</label>
										<div class="ipttxt ipttxt-b wh-txtb">
											<input class="inipttxt" maxlength="4" name="multiple" size="4" type="text" value="1" />
										</div>
										<label class="lbl">金额：</label>
										<div class="tips tipsBetAmount">
											<b _n="money" class="c-h">0</b>元
										</div>
										<label class="lbl">理论最高奖金：</label>
