#personal-data{
	width: 730px;
	height: 725px;
	margin: 0 auto;
}
#personal-data .personal-data-title{
	width: 500px;
	height: 26px;
	line-height: 26px;
	margin: 0 auto;
	border-bottom: 1px #EAEAEA solid;
	font-size: 14px;
}
.personal-data-form{
	width: 730px;
	height: 660px;
}
.personal-data-form li{
	height: 34px;
	width: 730px;
	line-height: 34px;
	margin-top: 15px;
}
.personal-data-form li label{
	float:left;
	width:100px;
	height: 34px;
	line-height:34px;
	text-align:right;
	font-size:14px;
	margin-right: 10px;
	color: #000000;
	font-family: "微软雅黑";

}
.personal-data-form li select{
	min-width:90px;
	height:34px;
	line-height:34px;
	font-size:14px;
	color: #000000;
	margin-right: 22px;
	font-family: "微软雅黑";
	
}
.personal-data-form li .upload-picture-button{
	height:34px;
	font-size:14px;
	cursor: pointer;
	padding-top: -5px;
	font-family: "微软雅黑";
}
.personal-data-form li .upload-picture-file-index{
	display: inline-block;
	width: 102px;
	height: 40px;
	overflow: hidden;
	background:url(../images/select-file.png) no-repeat;
}
.personal-data-form li .upload-picture-file-index input{
	filter:alpha(opacity=0.5); 
	-moz-opacity:0.5;              /* 老版Mozilla */
    -khtml-opacity:0.5;              /* 老版Safari */
    opacity: 0.5;
    width: 100px;
	height: 38px;
	font-size:1000px;
}
.personal-data-form li.personal-data-form-explain{
	height: 12px;
	width: 730px;
	line-height: 12px;
	margin-top: 6px;
	font-size: 12px;
	font-family: "微软雅黑";
	color: #999999;
}
.personal-data-form li.margin-top{
	margin-top:20px;
	}
.personal-data-form li.personal-data-form-preview{
	width: 730px;
	height: 180px;
}

.personal-data-form li .personal-data-form-previewimg{
	width: 106px;
	height: 106px;
	border: 1px #AFCBDF solid;
	text-align: center;
	float: left;
	margin:30px 80px 50px 120px;
	background-color: #CEDAE4;
}
.personal-data-form li .personal-data-form-previewimg  img{
	width: 106px;
	height: 106px;
}
.personal-data-form li .personal-data-form-thumbnailimg{
	width: 130px;
	height: 120px;
	float: left;
	margin-top:10px;
	margin-left: 20px;
}
.personal-data-form li .personal-data-form-thumbnailimg p{
	border-bottom: 1px #EAEAEA solid;
	height: 30px;
	width: 125px;
	line-height: 30px;
	font-size: 14px;
	font-family: "微软雅黑";
	margin-bottom: 10px;
}
.personal-data-form li .personal-data-form-thumbnailimg  img{
	width: 60px;
	height: 60px;
}
.personal-data-form li.long{
	width:730px;
	height: 200px;
}
.personal-data-form li textarea{
	width: 525px;
	height: 180px;
	border: 1px #CCCCCC solid;
}
.personal-data-form li .personal-data-form-nextbutton{
	 width: 120px;
	 height: 38px;
	 background: #8FD33F;
	 color: #fff;
	 font-size: 20px;
}

#attention-kind{
	width: 985px;
