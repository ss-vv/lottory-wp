	width: 540px;
	height: 30px;
	line-height: 30px;
	margin: 10px auto 60px;
}
.regist-guide-progress-bar span{
	float: left;
	font-size: 14px;
	
}
.regist-guide-progress-bar .span01{
	margin-left: 48px;
}
.regist-guide-progress-bar .span02{
