#attention-kind .attention-kind-title{
	width: 950px;
	height: 30px;
	line-height: 30px;
	margin: 10px auto;
	border-bottom: 4px #ECECEC solid;
}

/*下面这两部分是总选的复选框的设置*/
#attention-kind .attention-kind-title span.checkbox-all-selected{
	width: 55px;
	height: 30px;
	display: inline-block;
	margin-top: 9px;
	float: left;
	cursor: pointer;
	background:url(../images/checkbox-selected.png) no-repeat ;
}

/*#attention-kind .attention-kind-title span.checkbox-all:hover{
	background:url(../images/checkbox-not-selected.png) no-repeat ;
}*/


