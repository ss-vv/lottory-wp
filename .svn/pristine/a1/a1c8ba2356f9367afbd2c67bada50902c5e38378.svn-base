	background:url(../images/checkbox-not-selected.png) no-repeat ;
}

#attention-kind .attention-kind-title span{
	font-family: "微软雅黑";
	font-size: 16px;
}
#attention-kind .attention-kind-title  p{
	display: inline-block;
	float: right;
	margin-right: 50px;
	vertical-align: middle;
}
#attention-kind .attention-kind-title  p label{
	font-family: "微软雅黑";
	font-size: 14px;
	margin-left: 22px;
