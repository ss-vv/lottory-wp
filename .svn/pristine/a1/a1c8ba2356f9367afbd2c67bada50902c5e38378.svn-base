	float: left;
	margin-top: -8px;
	cursor: pointer;
}
#attention-kind .attention-kind-title  p input{
	margin-top: 3px;
	vertical-align: middle;
}
.attention-kind-list {
	clear:both ;
	width:980px;
	height: 140px;
	margin: 0 auto;
	border-bottom: 1px #AAA9A9 dashed;
}

.attention-kind-list li{
	width: 240px;
	height: 140px;
	float:left;
	font-family: "微软雅黑";
