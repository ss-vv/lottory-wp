	text-align: center;
}

.jj-list .jj-list-b .jj-list-detail li .jj-list-detail-sp .jj-list-failure
	{
	display: inline-block;
	width: 25%;
	text-align: right;
}

.sp-colorg {
	color: rgb(214, 214, 214);
}