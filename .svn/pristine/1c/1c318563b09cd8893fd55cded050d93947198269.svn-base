.jj-list .jj-list-b .jj-list-detail li .jj-list-detail-sp .jj-list-victory
	{
	display: inline-block;
	width: 25%;
	text-align: left;
}

.jj-list .jj-list-b .jj-list-detail li .jj-list-detail-sp .jj-list-draw
	{
	display: inline-block;
