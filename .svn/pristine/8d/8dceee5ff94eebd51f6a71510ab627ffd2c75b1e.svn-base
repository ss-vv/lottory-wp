package com.davcai.lottery.platform.client.anruizhiying.model;

import java.util.List;

import org.apache.commons.lang3.builder.ReflectionToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;

/**
 * @author haoxiang.jiang@davcai.com
 * @date 2015年1月9日 下午2:56:34
 */
public class AnRuiZhiYingJCMatchModel {
	private String matchId;
	private String gameNo;
	private String gameName;
	private String homeName;
	private String awayName;
	private String gameTime;
	private List<AnRuiZhiYingJCPlayOddsModel> playOdds;
	
	public String getMatchId() {
		return matchId;
	}
	public void setMatchId(String matchId) {
		this.matchId = matchId;
	}
	public String getGameNo() {
		return gameNo;
	}
	public void setGameNo(String gameNo) {
		this.gameNo = gameNo;
	}
	public String getGameName() {
		return gameName;
	}
	public void setGameName(String gameName) {
		this.gameName = gameName;
	}
	public String getHomeName() {
		return homeName;
	}
	public void setHomeName(String homeName) {
		this.homeName = homeName;
	}
	public String getAwayName() {
		return awayName;
	}
	public void setAwayName(String awayName) {
		this.awayName = awayName;
	}
	public String getGameTime() {
		return gameTime;
	}
	public void setGameTime(String gameTime) {
		this.gameTime = gameTime;
	}
	public List<AnRuiZhiYingJCPlayOddsModel> getPlayOdds() {
		return playOdds;
	}
	public void setPlayOdds(List<AnRuiZhiYingJCPlayOddsModel> playOdds) {
		this.playOdds = playOdds;
	}
	public String toString(){
		return ReflectionToStringBuilder.toString(this, ToStringStyle.MULTI_LINE_STYLE);
	}
}
