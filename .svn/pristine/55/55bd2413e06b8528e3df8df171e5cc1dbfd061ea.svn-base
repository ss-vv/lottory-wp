package com.xhcms.lottery.dc.feed.web.action.groupbuy;

import com.xhcms.lottery.commons.data.BetScheme;
import com.xhcms.lottery.commons.data.UserScore;

/**
 * @author Pink
 *
 */
public class GroupBuyScheme {
	private BetScheme scheme;
	private UserScore userScore;
	
	public GroupBuyScheme(){}
	
	public GroupBuyScheme(BetScheme scheme, UserScore userScore) {
		this.scheme = scheme;
		this.userScore = userScore;
	}
	
	public BetScheme getScheme() {
		return scheme;
	}
	
	public void setScheme(BetScheme scheme) {
		this.scheme = scheme;
	}
	
	public UserScore getUserScore() {
		return userScore;
	}
	
	public void setUserScore(UserScore userScore) {
		this.userScore = userScore;
	}
}
