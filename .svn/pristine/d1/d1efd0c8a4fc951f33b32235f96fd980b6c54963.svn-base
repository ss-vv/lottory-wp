.arrow-top .arrow-top-out {
    position: absolute;
    width: 0;
    height: 0;
    top: 0;
    right: 6px;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    border-bottom: 6px solid #000000;
    content: "";
    opacity: 0.2;
    filter: alpha(opacity = 20);
    opacity: 0.2;
    filter: alpha(opacity = 20);
    z-index: 100
}

.arrow-top .arrow-top-in {
    position: absolute;
    width: 0;
    height: 0;
    top: 1px;
    right: 6px;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    border-bottom: 6px solid #ffffff;
    z-index: 101
}

