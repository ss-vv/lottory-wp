log4j.rootLogger = INFO,C
log4j.logger.org.apache = WARN
log4j.logger.org.hibernate = WARN
log4j.logger.org.springframework = WARN
log4j.logger.com.xhcms = DEBUG
log4j.logger.com.unison = DEBUG
log4j.logger.com.unison.lottery.mc.uni = DEBUG
log4j.logger.com.xhcms.lottery.mc.jc.handler.JCHistoryTicketChecker = INFO,C

# \u8bb0\u5f55\u51fa\u7968\u548c\u4e2d\u5956\u76f8\u5173\u4fe1\u606f
log4j.additivity.com.xhcms.lottery.mc.persist.service.impl = false
log4j.logger.com.xhcms.lottery.mc.persist.service.impl = INFO,C

# \u5f00\u53d1\u73af\u5883
log4j.appender.C = org.apache.log4j.ConsoleAppender
log4j.appender.C.Target = System.out
log4j.appender.C.layout = org.apache.log4j.PatternLayout
log4j.appender.C.layout.ConversionPattern = [%-d{HH:mm:ss} %c{2}] %m%n

