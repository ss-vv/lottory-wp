/*下面开始davcai-header头部导航部分*/
.davcai-header{
	width: 100%;
	height: 66px;
	line-height: 66px;
    background:url(../../../images/davcai/header/davcai-header-bg.png) repeat-x ;	
    margin: 0 auto;
    position: fixed; 
    left: 0;
    top: 0;            /*这段代码里面的 position: fixed;是为了使头部导航固定不变。*/
    z-index:999999;
}
.davcai-header .davcai-header-content{
	width: 985px;
	height: 66px;
	line-height: 66px;
	margin: 0 auto;
}
#davcai-logo{
	float: left;
}
#davcai-logo a{
	width: 165px;
	height: 110px;
	background:url(../../../images/davcai/header/davcai-logo.png);
    display: inline-block;
 }
.davcai-header-content .davcai-address{
	width: 125px;
	height: 55px;
	float: left;
	background:url(../../../images/davcai/header/dacom.png) 5px 18px no-repeat;
	padding-top: 15px;
	color: #FFF113;
	padding-left: 2px;
	margin-right: 15px;
}
.davcai-header-content .davcai-address a.huoban{
 	font-size: 11px;
	font-family: "微软雅黑";
	color:#FFF113;
 }
.davcai-header-menu li{
	float: left;
	height: 66px;
	line-height: 66px;
    margin-left: -15px;
}

.davcai-header-content form{
