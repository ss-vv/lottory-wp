package com.xhcms.lottery.commons.exception;

/**
 * 追号异常。
 * 
 * @author Yang Bo
 */
public class RepeatPlanException extends Exception {

	private static final long serialVersionUID = 4011851992713800475L;

	public RepeatPlanException() {
	}

	public RepeatPlanException(String message) {
		super(message);
	}

	public RepeatPlanException(Throwable cause) {
		super(cause);
	}

	public RepeatPlanException(String message, Throwable cause) {
		super(message, cause);
	}
}
