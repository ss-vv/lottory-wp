package com.unison.lottery.weibo.dataservice.parse.model;

import org.apache.commons.lang3.builder.ReflectionToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;

public class AsiaOdd  extends ConcedeOdd{
	
	private boolean fengPan;//是否封盘
	private boolean zouDi;//是否走地
	public boolean isFengPan() {
		return fengPan;
	}
	public void setFengPan(boolean fengPan) {
		this.fengPan = fengPan;
	}
	public boolean isZouDi() {
		return zouDi;
	}
	public void setZouDi(boolean zouDi) {
		this.zouDi = zouDi;
	}
	
	public String toString(){
		return ReflectionToStringBuilder.toString(this,
				ToStringStyle.MULTI_LINE_STYLE);
	}

}
