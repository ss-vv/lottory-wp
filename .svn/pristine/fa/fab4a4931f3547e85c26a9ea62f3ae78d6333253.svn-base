package com.unison.lottery.weibo.statistic.constant;

public class StatisticKeys {
	private static final String officialUserData = "officialUserDataKey:%s";
	public static String getOfficialUserDataKey(String officialUserId){
		return String.format(officialUserData, officialUserId);
	}
	
	private static final String  officialUser = "officialUser";
	public static String getOfficialUserKey(){
		return officialUser;
	}
	
	private static final String  realWeiboIndex = "realWeiboIndex";
	public static String getRealweiboindexKey() {
		return realWeiboIndex;
	}
}
