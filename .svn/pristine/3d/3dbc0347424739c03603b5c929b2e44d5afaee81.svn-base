package com.unison.lottery.weibo.dataservice.commons.parse;


/**
 * 文本文档，定义一个文本串的格式。由 {@link ExtractEngine} 负责解析。
 * 
 * @author Yang Bo
 */
public class TextDocument extends TextField {
	
	// 文档使用的日期格式
	private String dateFormat;	// 日期串格式，不为null会被注册到 ConvertUtils 中。
	
	public String getDateFormat() {
		return dateFormat;
	}

	public void setDateFormat(String dateFormat) {
		this.dateFormat = dateFormat;
	}
}
