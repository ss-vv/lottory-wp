log4j.rootLogger = INFO,R,C
log4j.logger.org.apache = WARN
log4j.logger.org.hibernate = WARN
log4j.logger.org.springframework = WARN
log4j.logger.com.unison = INFO
log4j.logger.com.unison.lottery.pm.data.OrderColorGrant = INFO,orderColor

log4j.appender.C = org.apache.log4j.ConsoleAppender
log4j.appender.C.Target = System.out
log4j.appender.C.layout = org.apache.log4j.PatternLayout
log4j.appender.C.layout.ConversionPattern = %-d{HH:mm:ss} %p [%c] - %m%n

log4j.appender.R=org.apache.log4j.DailyRollingFileAppender
log4j.appender.R.File=/data/logs/pm.log
log4j.appender.R.DatePattern = '.'yyyy-MM-dd
log4j.appender.R.layout=org.apache.log4j.PatternLayout
log4j.appender.R.layout.ConversionPattern=%d{HH:mm:ss} %p [%c] - %m%n

log4j.appender.orderColor=org.apache.log4j.DailyRollingFileAppender
log4j.appender.orderColor.File=/data/logs/pm_orderColorRing_Grant.log
log4j.appender.orderColor.DatePattern = '.'yyyy-MM-dd
log4j.appender.orderColor.layout=org.apache.log4j.PatternLayout
log4j.appender.orderColor.layout.ConversionPattern=%d{HH:mm:ss} %p [%c] - %m%n