package com.xhcms.lottery.commons.persist.service;

import com.xhcms.lottery.commons.exception.JXRuntimeException;

/**
 * 投注时的异常。
 * 
 * @author yangbo
 */
public class BetException extends JXRuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = -1241234061145205928L;

	public BetException(String msg, int errorCode) {
		super(msg, errorCode);
		
	}

	public BetException(String msg, Throwable cause) {
		super(msg, cause);
		
	}

	public BetException(String msg) {
		super(msg);
		
	}
	
}
