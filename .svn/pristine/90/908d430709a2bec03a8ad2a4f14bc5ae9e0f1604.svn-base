.davcai-header{
	width: 100%;
	height: 66px;
	line-height: 66px;
    background:url(../../../images/davcai/header/davcai-header-bg.png) repeat-x ;	
    margin: 0 auto;
    position: fixed; 
    left: 0;
    top: 0;            /*这段代码里面的 position: fixed;是为了使头部导航固定不变。*/
    z-index:9999;
}
.davcai-header .davcai-header-content{
	width: 985px;
	height: 66px;
	line-height: 66px;
	margin: 0 auto;
}
#davcai-logo{
	float: left;
}
#davcai-logo a{
	width: 165px;
	height: 110px;
	background:url(../../../images/davcai/header/davcai-logo.png);
    display: inline-block;
 }
.davcai-header-content .davcai-address{
	width: 125px;
	height: 55px;
	float: left;
	background:url(../../../images/davcai/header/dacom.png) 5px 18px no-repeat;
	padding-top: 15px;
	color: #FFF113;
	padding-left: 2px;
	margin-right: 15px;
}
.davcai-header-content .davcai-address a.huoban{
 	font-size: 11px;
	font-family: "微软雅黑";
	color:#FFF113;
 }
.davcai-header-menu li{
	float: left;
	height: 66px;
	line-height: 66px;
    margin-left: 0px;
}
.davcai-header-menu li a{
	color: #ffffff;
	font-size: 16px;
	font-family: "微软雅黑";
	display: block;
	width: 50px;
	height: 64px;
	text-align: center;
}
.davcai-header-menu li a:hover{
	background-color:#CE2219 ;
}
.davcai-header-menu li a.current{
	background-color:#CE2219 ;
}
.davcai-header-content .mail-box{
	width: 40px;
	height: 40px;
	float: left;
	margin-left: 30px;
}
.davcai-header-content .mail-box > a{
	background: url(http://www.davcai.com/assets/js/bet/images/mail.png) no-repeat left bottom;
	width: 22px;
	height: 14px;
	display: block;
	margin-top: 26px;
}
.mail-box-nullbg{
	width: 40px;
	height: 40px;
	float: left;
	margin-left: 30px;
	
}
.mail-box i.mail-tip{
	width: 16px;
	height: 16px;
	background:url(../../../images/davcai/header/mail-tip.png) no-repeat;
	display: block;
	float: left;
	margin-top: 15px;
	margin-left: 13px;
}
.mail-box .mail-num{
	float: left;       /*这个.mail-num使用的是障眼法，只能限制于两位数字，再议*/       
	width: 16px;
	height: 16px;
	font-size: 11px;
	font-family: arial;
	display: inline-block;
	margin-top: -40px;
	margin-left: 14px;
}
.mail-box .mail-num a{
	color: #000000;
}
