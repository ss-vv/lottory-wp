package com.davcai.lottery.platform.util.model;

import java.util.List;

import org.apache.commons.lang3.builder.ReflectionToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;

import com.xhcms.lottery.commons.data.Ticket;

public class PriorityListAndTicketsToAllocate {
	private List<LotteryPlatformPriority> priorityList;
	private List<Ticket> ticketsToAllocate;
	public List<LotteryPlatformPriority> getPriorityList() {
		return priorityList;
	}
	public void setPriorityList(List<LotteryPlatformPriority> priorityList) {
		this.priorityList = priorityList;
	}
	public List<Ticket> getTicketsToAllocate() {
		return ticketsToAllocate;
	}
	public void setTicketsToAllocate(List<Ticket> ticketsToAllocate) {
		this.ticketsToAllocate = ticketsToAllocate;
	}
	
	public String toString(){
		return ReflectionToStringBuilder.toString(this, ToStringStyle.MULTI_LINE_STYLE);
	}

}
