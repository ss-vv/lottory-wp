package com.unison.lottery.weibo.data;

import java.io.Serializable;


public class Favourite implements Serializable{

	private static final long serialVersionUID = 7079688397485903503L;
	
	private WeiboMsgVO weiboMsg;
	
	// 格式化后的收藏时间
	private String favouriteTimeFmt;

	public WeiboMsgVO getWeiboMsg() {
		return weiboMsg;
	}

	public void setWeiboMsg(WeiboMsgVO weiboMsg) {
		this.weiboMsg = weiboMsg;
	}

	public String getFavouriteTimeFmt() {
		return favouriteTimeFmt;
	}

	public void setFavouriteTimeFmt(String favouriteTimeFmt) {
		this.favouriteTimeFmt = favouriteTimeFmt;
	}
}
