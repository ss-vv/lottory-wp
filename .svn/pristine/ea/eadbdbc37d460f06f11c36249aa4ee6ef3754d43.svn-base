package com.unison.lottery.weibo.dataservice.parse.model;

import java.util.Date;

import org.apache.commons.lang3.builder.ReflectionToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;

public class MatchAgenda {

	// 比赛ID
	private String matchId;
	// 颜色值
	private String colour;
	// 赛事类型 简体名
	// 赛事类型 繁体名
	// 赛事类型 英文名
	private BaseName matchTypeName;
	// 主队 简体名
	// 主队 繁体名
	// 主队 英文名
	private BaseName homeName;
	// 客队 简体名
	// 客队 繁体名
	// 客队 英文名
	private BaseName awayName;
	// 比赛时间 只有小时和分数 20:30 格式
	private String matchTimeWithHourAndMin;
	// 开场时间 (上半场时为开上半场的时间,下半场时为开下半场的时间）js日期时间格式
	private Date matchBeginTime;
	private Date matchBeginTimeInGMT0;
	// 比赛状态 0:未开,1:上半场,2:中场,3:下半场,4,加时，-11:待定,-12:腰斩,-13:中断,-14:推迟,-1:完场，-10取消
	private String matchStatus;
	// 主队比分
	private String homeScores;
	// 客队比分
	private String awayScores;
	// 主队上半场比分
	private String homeHalfScores;
	// 客队上半场比分
	private String awayHalfScores;
	// 主队红牌
	private String homeRedCards;
	// 客队红牌
	private String awayRedCards;
	// 主队黄牌
	private String homeYellowCards;
	// 客队黄牌
	private String awayYellowCards;
	// 主队排名
	private String homeRanking;
	// 客队排名
	private String awayRanking;
	// 是否是精简版比分 0 /1表示，1表示精简版（即重要赛事）
	private String isAllOrSimple;
	// 是否是足彩比分 0：非足彩，1:胜负彩，2：北京单场，3：胜负彩+北京单场
	private String isFootballLotteryScore;
	// 电视直播频道: CCTV5 广东体育
	private String TVLive;
	// 是否有阵容: 1为有
	private String isHaveBattleArray;
	// 是否是走地比赛 True or False
	private boolean zouDi;
	// 皇冠初盘
	private String crownPrimaryDisc;
	// 比赛说明
	private String matchMessage;
	// 联赛资料库超链接 (可不理会)
	private String leagueDataUrl;
	// 空(有变动)
	private String customOne;
	// 空(有变动)
	private String customTwo;
	// 空(有变动)
	private String customThree;
	// 空(有变动)
	private String customFour;
	// 比赛日期 m-d， 月-日
	private String matchTimeWithMonthAndDay;
	// 主队ID
	private String homeTeamId;
	// 客队ID
	private String awayTeamId;
	// 空(有变动)
	private String  customFive;
	// 国家ID (有变动)
	private String countryId;
	
	//是否中立场 true or false
	private String isCenter;
	
	public String toString() {
		return ReflectionToStringBuilder.toString(this,
				ToStringStyle.MULTI_LINE_STYLE);
	}
	
	
	public String getIsCenter() {
		return isCenter;
	}


	public void setIsCenter(String isCenter) {
		this.isCenter = isCenter;
	}

	public String getMatchId() {
		return matchId;
	}

	public void setMatchId(String matchId) {
		this.matchId = matchId;
	}

	public String getColour() {
		return colour;
	}

	public void setColour(String colour) {
		this.colour = colour;
	}

	public BaseName getMatchTypeName() {
