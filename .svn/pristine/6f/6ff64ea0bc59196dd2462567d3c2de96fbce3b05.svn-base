@charset "utf-8";/* init */body {margin:0px; padding:0px; font-family:"宋体"; font-size:12px; line-height:24px; color:#333; word-break:break-all;}a, b, cite, div, em, form, i, img, label, p, span, strong, var,h1, h2, h3, h4, h5, h6,dl, dt, dd,table, thead, tbody, tr, th, td {margin:0px; padding:0px; border:none;}a:hover {text-decoration:underline;}a {color:#333; text-decoration:none;}.fl {display:inline; float:left;}.fr {display:inline; float:right;}.ofh {overflow:hidden; zoom:1;}.crb {clear:both;}.brrn {border:none!important;}.bgn {background:none!important;}.fs14 {font-size:14px!important;}.fwb {font-weight:bold!important;}.c01 {color:#f30;}.c02 {color:#999;}.c03 {color:#69c;}.tanc {text-align:center;}.tanl {text-align:left;}.tanr {text-align:right;}/* ios */.ios {font-size:0px; line-height:9999px; float:left; overflow:hidden;}.ios16 {width:16px; height:16px; background:url(../images/ios16.gif) no-repeat;}.ios16_arwb01 {background-position:-128px 0px;}.ios16_msge {background-position:-16px 0px;}.ios16_phone {background-position:-32px 0px;}.ios16_unlock {background-position:-48px 0px;}.ios16_yes {background-position:-64px 0px;}.ios16_no {background-position:-80px 0px;}.ios16_lock {background-position:-96px 0px;}.ios16_note {background-position:-112px 0px;}.ios32 {width:32px; height:32px; background:url(../images/ios32.gif) no-repeat;}.ios32_no {background-position:-32px 0px;}.ios32_mail {background-position:-64px 0px;}.ios32_tel {background-position:-96px 0px;}.ios32_psn {background-position:-128px 0px;}/* iptTxt */.iptTxt {display:inline; float:left;}.iptTxt .iniptTxt {float:left;}.iptTxt01 {width:130px; height:24px; background:#ebebeb;}.iptTxt01 .iniptTxt {width:123px; height:17px; margin:2px 0px 0px 2px; padding:1px 0px 0px 1px; background:#fff; border:1px solid; border-color:#999 #e2e2e2 #e2e2e2 #999;}.iptTxt02 .iniptTxt {width:157px; height:17px; padding:3px 0px 0px 3px; background:#fff; border:#d3d3d3 1px solid;}.iptTxt03 .iniptTxt {width:97px; height:17px; padding:3px 0px 0px 3px; background:#fff; border:#d3d3d3 1px solid;}.iptTxt04 .iniptTxt {width:320px; height:17px; padding:3px 0px 0px 3px; background:#fff; border:#d3d3d3 1px solid;}.iptTxt05 .iniptTxt {width:145px; height:15px; color:#999; padding:1px 0px 0px 3px; background:#fff; border:1px solid; border-color:#c7c8c9 #e7e8e8 #e6e7e7 #c4c5c6;}.iptTxt_ate .iniptTxt {border-color:#fc0;}/* slt */.slt {cursor:pointer; display:inline; float:left;}.slt .inslt {text-align:center; float:left; overflow:hidden;}.slt01 {padding:2px; margin-left:30px; background:url(../images/noRpt.gif) no-repeat -160px 0px; cursor:pointer; display:inline; float:left;}.slt01 .inslt {width:54px; height:18px; line-height:18px; text-align:center; background:#fff; border-top:#999 1px solid; border-left:#999 1px solid; border-bottom:#e2e2e2 1px solid; float:left; overflow:hidden;}.slt01 .ios16_arwb:hover {background-color:#f8f8f8;}.slt01 .ios16_arwb {height:18px; background-color:#fff; border-top:#999 1px solid; border-right:#e2e2e2 1px solid; border-bottom:#e2e2e2 1px solid;}/* slt_l */.slt_l {width:70px; margin:0px; padding:0px; background:#fff; border:#e2e2e2 1px solid; position:absolute; left:32px; top:21px; list-style:none; display:none; overflow:hidden; zoom:1; clear:both;}.slt_l li {width:100%; height:20px; line-height:20px; text-align:left; text-indent:9px; cursor:pointer; white-space:nowrap; display:inline; float:left; overflow:hidden; zoom:1; clear:both;}/* iptCkx */.iptCkx {display:inline; float:left;}.iptCkx .iniptCkx,.iptCkx span {float:left;}.iptCkx .iniptCkx {margin:5px 5px 4px 0px; margin:2px 2px 1px -3px\9;/*ie8*/ *margin:1px 2px 0px -4px;/*ie7*/ _margin:1px 2px 0px -4px;/*ie6*/}.iptCkx01 {margin-left:50px;}/* iptRdo */.iptRdo {display:inline; float:left;}.iptRdo .iniptRdo,.iptRdo span {float:left;}.iptRdo .iniptRdo {margin:5px 5px 4px 0px; margin:2px 2px 1px -3px\9;/*ie8*/ *margin:1px 2px 0px -4px;/*ie7*/ _margin:1px 2px 0px -4px;/*ie6*/}/* ttaa */.ttaa {display:inline; float:left;}.ttaa .inttaa {float:left; overflow:hidden; overflow-y:scroll;}.ttaa01 .inttaa {width:320px; height:50px; font-size:12px; padding:3px 0px 0px 3px; background:#fff; border:#d3d3d3 1px solid;}.ttaa02 .inttaa {width:320px; height:100px; font-size:12px; padding:3px 0px 0px 3px; background:#fff; border:#d3d3d3 1px solid;}/* btn */.btn {text-align:center; background:url(../images/btn01.gif) no-repeat; border:none; cursor:pointer; float:left; overflow:hidden;}.btn01 {width:128px; height:38px; font-size:0px; line-height:999999px;}a.btn02:hover {background-position:-356px 0px;}.btn02 {width:70px; height:28px; line-height:28px; background-position:-276px 0px;}a.btn03:hover {background-position:-111px -48px;}.btn03 {width:101px; height:34px; line-height:34px; background-position:0px -48px;}.btn04 {width:59px; height:18px; line-height:18px; padding-left:20px; background-position:-222px -48px;}/* box01 */.box01 {width:610px; margin:0px auto; padding:20px 20px 10px; overflow:hidden; zoom:1;}.box01 .btn03 {margin:0px auto; display:block; float:none;}.box02 {width:948px; height:23px; margin:0px auto 15px; background:#fff; border:#ececec 1px solid; border-top:none;}/* pmt */.pmt01 {width:240px; height:32px; line-height:32px; font-size:14px; font-weight:bold; margin:50px auto;}.pmt01 .ios32_yes {margin-right:10px; _margin-right:7px;}/* bg */.bg01 {background:#fef6f6; border:#fdefef 1px solid;}.bg02 {background:#f7fbf5; border:#eff4ec 1px solid;}.bg03 {background:#f9f5fb; border:#f2e9f6 1px solid;}.bg04 {background:#fbf5f7; border:#f6eaed 1px solid;}/* brr */.brr01 {background:url(../images/brr01.gif) repeat-x left bottom;}/* topbar */.topbar {width:950px; height:27px; line-height:27px; margin:0px auto; background:url(../images/topbar.gif) repeat-x;}.logo {width:79px; height:23px; margin:2px 10px 0px 9px;}.helpbar a {color:#f90; margin-right:10px;}.menu {padding:0px 10px;}.topbar .qikmenu {margin-top:4px;}/* qikmenu */.qikmenu {background:url(../images/qikmenu.gif) repeat-x; border:#cacaca 1px solid; cursor:pointer; display:inline; float:left; position:relative;}.qikmenu span {height:16px; line-height:17px; padding:0px 10px; border-left:#f4f4f4 1px solid; border-right:#cacaca 1px solid; display:inline; float:left; overflow:hidden; zoom:1;}.qikmenu b {width:14px; border-left:#f4f4f4 1px solid; border-right:#f4f4f4 1px solid;}/* quickMenu*/.qucikMenuContent {width:62px; margin:0px; padding:0px; background:#f8f8f8; border:#cacaca 1px solid; border-bottom:none; position:absolute; top:16px; left:-1px; list-style:none; display:none; overflow:hidden; z-index:1;}.qucikMenuContent a:hover {text-decoration:none; background:#fff;}.qucikMenuContent a {height:24px; padding-left:5px; border-bottom:#cacaca 1px solid; display:block; overflow:hidden;}/* ttnews */.ttnews {width:60%; padding:0px 10px; overflow:hidden;}.ttnews a {color:#2b83e6; margin-right:18px;}/* searchbar */.searchbar {margin:3px 5px 0px 0px;}.searchbar .qikmenu {margin:0px 5px;}/* login */.login {width:558px; margin:100px auto;}/* mod */.mod01 .mh {padding-left:8px; background:url(../images/trlbBG.gif) no-repeat;}.mod01 .mhl {padding-right:8px; background:url(../images/trlbBG.gif) no-repeat right top;}.mod01 .mhr {height:8px; background:#fff url(../images/trlbBG_t.jpg) repeat-x; overflow:hidden;}.mod01 .mm {padding-right:4px; background:#fff url(../images/trlbBG_r.jpg) repeat-y right top;}.mod01 .mml {padding-left:4px; background:url(../images/trlbBG_l.jpg) repeat-y;}.mod01 .mf {padding-left:8px; background:url(../images/trlbBG.gif) no-repeat left bottom;}.mod01 .mfl {padding-right:8px; background:url(../images/trlbBG.gif) no-repeat right bottom;}.mod01 .mfr {height:8px; background:#fff url(../images/trlbBG_b.jpg) repeat-x left 4px; overflow:hidden;}.mod02 {width:650px; margin:0px auto; padding:20px 0px;}.mod02 .mh {height:24px; padding:0px 10px; background:#f0f1f1; border-top:#f9f9f9 1px solid; border-bottom:#f6f7f7 1px solid;}.mod02 .mh h2 {color:#69c; font-size:12px; font-weight:bold; display:inline; float:left;}.mod02 .mh a {color:#999;}.mod03 .mh {height:24px; margin-top:15px; padding:0px 10px; border-bottom:#f90 1px solid;}.mod03 .mh h2 {color:#f90; font-size:14px; font-weight:bold; display:inline; float:left;}.mod03 .mm {margin-top:10px; overflow:hidden; zoom:1;}.mod03 .bg01 {height:460px;}.mod03 .bg02 {height:310px;}/* lgnbox */.lgnbox {width:282px;}.lgnbox .mml {height:317px; position:relative;}.lgnbox .mtt {width:48px; height:18px; background:url(../images/noRpt.gif) no-repeat; font-size:0px; line-height:9999px; position:absolute; left:66px; top:-29px; z-index:20; overflow:hidden;}.lgnbox .rbcdoll {width:92px; height:127px; background:url(../images/noRpt.gif) no-repeat -58px 0px; position:absolute; left:-30px; top:-50px; z-index:10; display:block; overflow:hidden;}/* fgtbox */.fgtbox {width:276px;}.fgtbox .mml {height:150px;}/* regebox */.regebox {width:276px;}.regebox .mml {height:151px;}/* wrap */.wrap {width:950px; margin:0px auto;}.col_l {width:414px; margin-left:40px;}.col_r {width:414px; margin-right:40px;}.tabh {overflow:hidden; zoom:1;}.tabh a,.tabh span {float:left; overflow:hidden;}.tabh a:hover {text-decoration:none;}.tabh01 a,.tabh01 span {height:25px; line-height:25px; font-weight:bold; text-align:center; margin-right:-12px;}.tabh01 span span {margin-right:0px;}.tabh01 .lcrbg_c {width:222px; background:url(../images/tabh01.gif) repeat-x 0px -140px;}.tabh01 .lcrbg_l {width:16px; background:url(../images/tabh01.gif) no-repeat 0px -35px;}.tabh01 .lcrbg_r {width:16px; background:url(../images/tabh01.gif) no-repeat 0px 0px;}.tabh01 .on .lcrbg_c {color:#fff; background:url(../images/tabh01.gif) repeat-x 0px -175px;}.tabh01 .on .lcrbg_l {width:16px; background:url(../images/tabh01.gif) no-repeat 0px -105px;}.tabh01 .on .lcrbg_r {width:16px; background:url(../images/tabh01.gif) no-repeat 0px -70px;}.tabh02 {height:auto; padding-bottom:31px; background:#f5f7f7; border:#dfdfdf 1px solid; overflow:visible; zoom:0;}.tabh02 .lcrbg_c {height:27px; line-height:27px; font-weight:bold; text-align:center; margin:5px 0px 0px 100px; padding:0px 30px;}.tabh02 .lcrbg_r {width:5px; height:27px; margin-top:4px; background:url(../images/tabh02.gif) no-repeat; display:none; overflow:hidden;}.tabh02 a:hover .lcrbg_c,.tabh02 .on .lcrbg_c {margin-top:4px; background:#fff; border:#dfdfdf 1px solid; border-bottom:none;}.tabh02 a:hover .lcrbg_c {padding:0px 29px;}.tabh02 .on:hover .lcrbg_c,.tabh02 .on .lcrbg_c {padding:0px 30px;}.tabh02 .on .lcrbg_r {display:inline;}.tabm_tt {padding:5px 10px;}