package com.xhcms.lottery.utils;

/**
 * 工具类相关异常。
 * 
 * @author Yang Bo
 *
 */
public class UtilityException extends Exception {

	private static final long serialVersionUID = 2993603270698340951L;

	public UtilityException(String msg){
		super(msg);
	}
	
	public UtilityException(String msg, Throwable cause){
		super(msg, cause);
	}
}
